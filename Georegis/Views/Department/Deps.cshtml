@model List<Entities.Department>


<table>
    <tr>
        @foreach (var item in Model)
        {
        <td>
            @item.DepartmentCode
        </td>
        <td>
            @item.Title
        </td>
        <td>
            @item.Description
        </td>
        }
    </tr>
</table>
 
@Html.ActionLink("Новое подразделение","RegNewDep", "Department")
